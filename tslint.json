{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "array-type": false,
        "interface-name": false,
        "interface-over-type-literal": false,
        "no-console": false,
        "max-line-length": false,
        "member-access": false,
        "object-literal-sort-keys": false,
        "one-line": true,
        "trailing-comma": false
    },
    "rulesDirectory": []
}